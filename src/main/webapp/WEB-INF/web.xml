<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
         <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>
            /WEB-INF/context.xml
        </param-value>
    </context-param>
    <servlet>
        <servlet-name>registerJS</servlet-name>
        <servlet-class>demoJSP_Servlet.src.main.java.servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registerJS</servlet-name>
        <url-pattern>/registerJS</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EmailListServlet</servlet-name>
        <servlet-class>demo2.src.main.java.email.EmailListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EmailListServlet</servlet-name>
        <url-pattern>/emailList</url-pattern>

    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>formHtml.html</welcome-file>
        <welcome-file>formProcessing.jsp</welcome-file>
        <welcome-file>formRegisted.jsp</welcome-file>

    </welcome-file-list>
</web-app>
